[program:jenkins]
command=/usr/bin/jenkins
user=jenkins
directory=/var/lib/jenkins
autostart=true
autorestart=true
startsecs=10
stopsignal=INT
stdout_logfile=/var/log/jenkins/jenkins.out.log
stderr_logfile=/var/log/jenkins/jenkins.err.log
environment=JENKINS_HOME="/var/lib/jenkins",JENKINS_WEBROOT="/var/cache/jenkins/war",JAVA_OPTS="-Djava.awt.headless=true",JENKINS_PORT="8080"